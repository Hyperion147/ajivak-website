{
  "headers": [
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "rewrites": [
    {
       "source": "/(.*)", "destination": "/"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/static/(.*)", "headers": { "cache-control": "public,max-age=31536000,immutable" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
